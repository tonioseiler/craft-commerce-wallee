{
    "name": "furbo/craft-commerce-wallee",
    "description": "Craft Commerce Payment Gateway for Wallee",
    "type": "craft-plugin",
    "version": "2.3",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "commerce wallee"
    ],
    "support": {
        "docs": "https://github.com/tonioseiler/craft-commerce-wallee/blob/main/README.md",
        "issues": "https://github.com/tonioseiler/craft-commerce-wallee/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Furbo GmbH",
            "homepage": "https://www.furbo.ch"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "craftcms/cms": "^4.0.0",
        "craftcms/commerce": "^4.0.0",
        "wallee/sdk": "^3.1.4"
    },
    "autoload": {
        "psr-4": {
            "craft\\commerce\\wallee\\": "src/"
        }
    },
    "extra": {
        "name": "Commerce Wallee Gateway",
        "handle": "commerce-wallee",
        "developer": "Furbo GmbH",
        "developerUrl": "http://www.furbo.ch",
        "documentationUrl": "https://github.com/tonioseiler/craft-commerce-wallee",
        "changelogUrl": "https://github.com/tonioseiler/craft-commerce-wallee/blob/craft-v4/CHANGELOG.md",
        "components": {
            "commerceWalleeService": "craft\\commerce\\wallee\\services\\CommerceWalleeService"
        },
        "class": "craft\\commerce\\wallee\\CommerceWallee"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
